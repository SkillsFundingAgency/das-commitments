                    "appServicePlanName": {
                        "value": "[parameters('sharedBackEndAppServicePlanName')]"
                    },
                    "appServicePlanResourceGroup": {
                        "value": "[parameters('sharedEnvResourceGroup')]"
                    },
                    "subnetResourceId": {
                        "value": "[parameters('sharedBackEndSubnetResourceId')]"
                    },
                    "deployStagingSlot": {
                        "value": true
                    },
                    "appServiceAppSettings": {
                        "value": {
                            "array": [
