<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SFA.DAS.Commitments.Support.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WebRole name="SFA.DAS.Commitments.Support.SubSite" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="SimpleEndPoint" />
          <Binding name="Endpoint1" endpointName="SecureEndpoint" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="SimpleEndPoint" protocol="http" port="80" />
      <InputEndpoint name="SecureEndpoint" protocol="https" port="8080" certificate="SSLCert" />
    </Endpoints>
    <Certificates>
      <Certificate name="SSLCert" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
  </WebRole>
</ServiceDefinition>